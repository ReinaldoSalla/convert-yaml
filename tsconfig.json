{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ESNext",
    "sourceMap": false,
    "allowJs": true,
    "outDir": "./dist",
    "skipLibCheck": true
  }
}
